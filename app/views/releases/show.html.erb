<% @title="Рейтинг МАИИ" %>
<div class="flex">
  <div class="flex-col mx-4 my-4">
    <div class="flex-row">
      <div>
        <% cache "release_dropdown/#{@model_name}/#{@city}/#{@release.id}" do %>
          <%= render "release_dropdown" %>
        <% end %>
      </div>
    </div>

    <% if @paging.display? %>
      <div class="flex-row my-2">
        <% cache "release_paging/#{@model_name}/#{@release.id}/#{@city}/#{@paging.from}-#{@paging.to}" do %>
          <%= render "paging" %>
        <% end %>
      </div>
    <% end %>

    <div class="flex-row my-2">
      <% cache "release/#{@model_name}/#{@release.id}/#{@city}/#{@paging.from}-#{@paging.to}" do %>
        <%= render "release" %>
      <% end %>
    </div>

    <% if @paging.display? %>
      <div class="flex-row my-2">
        <% cache "release_paging/#{@model_name}/#{@release.id}/#{@city}/#{@paging.from}-#{@paging.to}" do %>
          <%= render "paging" %>
        <% end %>
      </div>
    <% end %>
  </div>
</div>